{
  "name": "react-component-catalog",
  "author": "Stefan Natter <https://natterstefan.me>",
  "license": "Apache-2.0",
  "version": "0.3.0",
  "description": "Manage a catalog (registry) of react-components easily.",
  "main": "dist/index.js",
  "files": ["dist"],
  "scripts": {
    "build": "babel src --ignore */*.test.js,**/*.test.js --out-dir dist",
    "build-umd": "npm run prebuild && webpack --mode=production",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run test && npm run build",
    "size": "size-limit",
    "test": "jest",
    "watch-test": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git update-index --again",
      "jest --findRelatedTests"
    ]
  },
  "size-limit": [
    {
      "limit": "6 KB",
      "path": "dist/index.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natterstefan/react-component-catalog.git"
  },
  "keywords": [
    "react-components",
    "react-registry",
    "react-catalog",
    "registry",
    "catalog",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/natterstefan/react-component-catalog/issues"
  },
  "homepage": "https://github.com/natterstefan/react-component-catalog#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-dom": "^16.8.4",
    "rimraf": "^2.6.3",
    "size-limit": "^0.21.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0",
    "react": "^16.8.4"
  }
}
