{
  "name": "react-component-catalog",
  "author": "Stefan Natter <https://natterstefan.me>",
  "license": "Apache-2.0",
  "version": "1.0.1",
  "description": "Manage a catalog (registry) of react-components easily.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "dist",
    "es",
    "esm",
    "lib"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-es && npm run build-esm && npm run build-umd",
    "build-cjs": "BABEL_ENV=cjs babel src --ignore */*.test.js,**/*.test.js --out-dir lib",
    "build-es": "BABEL_ENV=es babel src --ignore */*.test.js,**/*.test.js --out-dir es",
    "build-esm": "BABEL_ENV=esm babel src --ignore */*.test.js,**/*.test.js --out-dir esm",
    "build-umd": "BABEL_ENV=umd webpack --mode=production",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src",
    "prebuild": "rimraf dist && rimraf lib",
    "prepublishOnly": "npm run test && npm run build",
    "size": "size-limit",
    "test": "jest",
    "watch-test": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git update-index --again",
      "jest --findRelatedTests"
    ]
  },
  "size-limit": [
    {
      "limit": "6 KB",
      "path": "dist/index.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natterstefan/react-component-catalog.git"
  },
  "keywords": [
    "react-components",
    "react-registry",
    "react-catalog",
    "registry",
    "catalog",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/natterstefan/react-component-catalog/issues"
  },
  "homepage": "https://github.com/natterstefan/react-component-catalog#readme",
  "peerDependencies": {
    "react": ">=16.8.3 <17",
    "react-dom": ">=16.8.3 <17"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "7.0.0",
    "@types/jest": "^24.0.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-ns": "^0.4.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "size-limit": "^0.21.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0"
  }
}
