{
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "version": "2.1.0",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "next"
      ],
      "message": "chore(release): publish %s"
    },
    "publish": {
      "allowBranch": [
        "master",
        "next"
      ]
    }
  }
}
